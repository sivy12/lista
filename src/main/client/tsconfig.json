{
  "compilerOptions": {
    "target": "es5",
    "module": "amd",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "declaration": false
  },
  "exclude": [
    "bower_components",
    "node_modules",
    "typings",
    "custom_typings/angular-translate",
    "custom_typings/ng-file-upload"
  ]
}
